#include <stdio.h>
int main() {
    int i, n, x, sum = 0;
    float mean;
    printf("Enter n: ");
    scanf("%d", &n);
    printf("Enter %d numbers:\n", n);
    for(i = 1; i <= n; i++) {
        scanf("%d", &x);
        sum += x;
    }
    mean = sum / (float)n;
    printf("Sum = %d, Mean = %.2f\n", sum, mean);
    return 0;
}
